{
  "name": "react-express-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch-dev-client": "webpack-dev-server --config webpack/client.config.js --mode development",
    "watch-dev-server": "nodemon --config nodemon.json server/app.js",
    "watch": "npm-run-all --parallel watch-dev-client watch-dev-server",
    "build-server": "webpack-cli --display-error-details --config webpack/server.config.js --mode production",
    "build-client": "webpack-cli --display-error-details --config webpack/client.config.js --mode production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "html-webpack-plugin": "^4.4.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
